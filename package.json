{
  "name": "blog-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "frontend": "npm run dev --workspace=frontend",
    "backend": "npm run develop --workspace=backend",
    "dev": "npm-run-all --parallel frontend backend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}